<odoo>
  <record id="vsao_pos_config_form" model="ir.ui.view">
    <field name="name">vsao.pos.config.inherit</field>
    <field name="model">res.config.settings</field>
    <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form"/>
    <field name="arch" type="xml">
      <div class="col-12 col-lg-6 o_setting_box" title="Mostrar somente Clientes do PDV?">
        <div class="o_setting_left_pane">
          <xpath expr="//field[@name='module_pos_coupon']" position="after">
            <field name="vsao_POS_filter_Partner"/>
          </xpath>
        </div>
        <div class="o_setting_right_pane" id="pos-coupon">
            <label for="module_pos_coupon" string="Coupons &amp; Promotions"/>
            <div class="text-muted">
                Filtra Clientes no PDV
            </div>
        </div>
      </div>   
    </field>
  </record>
</odoo>


