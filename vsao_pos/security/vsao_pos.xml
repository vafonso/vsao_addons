<odoo>
    <data>
        <record id="vsao_pos_group_onw_contact" model="res.groups">
            <field name="name">Mostra contatos próprios ou pessoa física</field>
            <field name="category_id" ref="base.module_category_usability"/>
        </record>

        <record id="vsao_pos_group_all_contacts" model="res.groups">
            <field name="name">Mostra todos os contatos</field>
            <field name="category_id" ref="base.module_category_usability"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>

  <data noupdate="1">
    <record model="ir.rule" id="vsao_only_own_contacts_or_not_company">
      <field name="name">Seleciona contatos próprios ou apenas pessoa física para vendas</field>
      <field name="model_id" ref="base.model_res_partner"/>
      <field name="domain_force"> ['|', ('create_uid','=',user.id), ('is_company','=', 0)] </field>
      <field name="groups" eval="[(4,ref('base.group_user'))]"/>
    </record>
  </data>
</odoo>